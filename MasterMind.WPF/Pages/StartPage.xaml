<Page x:Class="MasterMind.WPF.Pages.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MasterMind.WPF"
      mc:Ignorable="d"
      Height="570" Width="480"
      Title="StartPage">
    <Grid>
        <Image  Margin="-15" Source="/Resources/start.png"></Image>
        <Button Command="{Binding StartCommand}" Opacity="0" Cursor="Hand" HorizontalAlignment="Left" Margin="343,511,0,0" VerticalAlignment="Top" Width="97" Height="39"/>
        <Button Command="{Binding CloseCommand}" Opacity="0" Cursor="Hand" HorizontalAlignment="Left" Margin="447,0,0,0" VerticalAlignment="Top" Width="23" Height="14"/>
    </Grid>

</Page>